    "def monotonic_incr(nums):\n",
    "    stack_incr = []\n",
    "\n",
    "    for num in nums:\n",
    "        while stack_incr and num < stack_incr[-1]:\n",
    "            stack_incr.pop()\n",
    "        stack_incr.append(num)\n",
    "        print(stack_incr)\n",
    "\n",
    "nums = [6,7,2,1,4,5,3,7]\n",
    "print(monotonic_incr(nums))"
   ]
  }
