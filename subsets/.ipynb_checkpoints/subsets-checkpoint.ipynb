   "source": [
    "def combs(nums):\n",
    "    res = [[]]\n",
    "    for el in nums:\n",
    "        n = len(res)\n",
    "        for i in range(n):\n",
    "            new = res[i][:]\n",
    "            new.append(el)\n",
    "            res.append(new)\n",
    "        \n",
    "    return res\n",
    "\n",
    "combs([1,5,3])"
   ]
  }
